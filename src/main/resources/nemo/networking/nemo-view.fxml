<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<BorderPane xmlns:fx="http://javafx.com/fxml"
            fx:controller="nemo.networking.NemoController"
            fx:id="menu" styleClass="main-panel">
        <top>
            <HBox styleClass="bar_menu">
                <Button  text="help" styleClass="btn"/>
                <Button  text="files" styleClass="btn"/>
                <Button  text="new" styleClass="btn" onAction="#top_new_action"/>
                <Button  text="computers" styleClass="btn" onAction="#top_computer_action"/>
                <Button  text="network device" styleClass="btn" onAction="#top_network_device_action" />
                <Button  text="network service" onAction="#top_network_service_action" styleClass="btn" />
                <Button  text="virtual machines" onAction="#top_vm_action" styleClass="btn"/>
                <Button  text="server" styleClass="btn" onAction="#top_server_action"/>
            </HBox>

        </top>
        <center>
            <Pane fx:id="center_menu"/>
        </center>

</BorderPane>
